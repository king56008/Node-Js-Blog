<div class="author">
    <h1 class="author_heading"> Hi, Im Kingdavid </h1>
    <p class="author___body"> Web developer and content creator</p>
    <marquee> Catching Cruise and Bulding Sites</marquee>
</div>

<img src="/img/image.jpg">
<section class="articles">
<h2 class="article_heading">Lastest Post</h2>
<ul class="article-ul">

    <% data.forEach(post => { %>
    <li>
        <a href="/post/<%= post._id %>">
            <Span><%= post.title %></Span>
            <span class="art_list"><%= post.createdAt.toDateString() %></span>
        </a>
    </li>
    <% }) %>
    
</ul>
<% if (nextPage !== null) { %>
    <a href="/?page=<%= nextPage %>" class="pagination">&lt; View Older Post</a>
<% } %>

</section>